<h1>Please login</h1>

<div>
<%= form_with model: @concierge do |f| %>
<div>
    <%= f.label :email, "Email:" %>
    <%= f.text_field :email %>
</div>

<div>
    <%= f.label :password, "Password:" %>
    <%= f.password_field :password %>
</div>

<div>
    <%= f.submit "Log In" %>
</div>

<% end %>
<div>

